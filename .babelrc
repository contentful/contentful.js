{
  "plugins": [
    "transform-object-rest-spread"
  ],
  "env": {
    "browser": {
      "presets": [
        ["env", {
          "targets": {
            "browsers": [
              "last 3 versions",
              "ie >= 11"
            ]
          },
          "modules": false
        }]
      ],
      "plugins": ["transform-object-assign"]
    },
    "node": {
      "presets": [
        ["env", {
          "targets": {
            "node": "4.7"
          },
          "modules": false
        }]
      ]
    },
    "test": {
      "presets": [
        ["env", {
          "targets": {
            "node": "4.7",
            "browsers": [
              "last 3 versions",
              "ie >= 11"
            ]
          }
        }]
      ],
      "plugins": ["rewire"],
      "sourceMaps": "inline"
    }
  }
}
