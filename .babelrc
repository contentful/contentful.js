{
  "env": {
    "webpack": {
      "presets": [
        ["env", {
          "targets": {
            "node": "4.7",
            "browsers": [
              "last 3 versions",
              "ie >= 11"
            ]
          },
          "modules": false
        }]
      ],
      "plugins": ["transform-object-assign"]
    },
    "test": {
      "presets": [
        ["env", {
          "targets": {
            "node": "4.7",
            "browsers": [
              "last 3 versions",
              "ie >= 11"
            ]
          }
        }]
      ],
      "plugins": ["rewire", "transform-object-assign"],
      "sourceMaps": "inline"
    }
  }
}
