{
  "name": "contentful",
  "description": "Client for Contentful's Content Delivery API",
  "version": "0.0.0-determined-by-semantic-release",
  "homepage": "https://www.contentful.com/developers/documentation/content-delivery-api/",
  "source": "./lib/index.ts",
  "main": "./dist/contentful.cjs.js",
  "module": "./dist/contentful.esm.js",
  "main-optimized": "./dist/contentful.cjs.min.js",
  "module-optimized": "./dist/contentful.esm.min.js",
  "exports": {
    "import": "./dist/contentful.esm.js",
    "require": "./dist/contentful.cjs.js"
  },
  "types": "./dist/types/index.d.ts",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "targets": {
    "types": false,
    "main": {
      "optimize": false,
      "outputFormat": "commonjs"
    },
    "module": {
      "optimize": false,
      "outputFormat": "esmodule"
    },
    "main-optimized": {
      "optimize": true,
      "outputFormat": "commonjs"
    },
    "module-optimized": {
      "optimize": true,
      "outputFormat": "esmodule"
    }
  },
  "tsd": {
    "directory": "test/types",
    "compilerOptions": {
      "rootDir": "./lib",
      "typeRoots": ["./lib"]
    }
  },
  "files": [
    "dist",
    "lib",
    "ADVANCED.md",
    "CONTRIBUTING.md",
    "README.md",
    "SETUP.md",
    "SECURITY.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/contentful/contentful.js.git"
  },
  "author": "Contentful <opensource@contentful.com>",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && parcel build --no-source-maps && tsc --emitDeclarationOnly",
    "build:dev": "npm run clean && parcel build && tsc --emitDeclarationOnly",
    "build:types": "tsc --declaration --emitDeclarationOnly",
    "postbuild": "npm run check:browser && npm run check:node && npm run test:size",
    "check:browser": "es-check es2019 --module ./dist/contentful.esm.js",
    "check:node": "es-check es2015 ./dist/contentful.cjs.js",
    "docs:build": "typedoc --options typedoc.json",
    "docs:watch": "npm run docs:build -- --watch",
    "docs:publish": "npm run docs:build && ./node_modules/contentful-sdk-jsdoc/bin/publish-docs.sh contentful.js contentful",
    "lint": "eslint \"lib/**\" \"test/**\"",
    "prettier": "prettier --write '**/*.{jsx,js,ts,tsx}'",
    "prettier:check": "prettier --check '**/*.{jsx,js,ts,tsx}'",
    "pretest": "rimraf coverage",
    "test": "npm run test:unit && npm run test:integration && npm run lint",
    "test:unit": "jest ./test/unit",
    "test:unit:watch": "jest ./test/unit --watch",
    "test:integration": "jest ./test/integration",
    "test:size": "bundlesize",
    "test:types": "./node_modules/.bin/tsd",
    "test:prepush": "npm run build && npm run test:unit && npm run test:size",
    "prepublishOnly": "npm run build",
    "postpublish": "npm run docs:publish && npm run clean",
    "semantic-release": "semantic-release",
    "precommit": "npm run lint && npm run prettier:check",
    "prepush": "npm run test:prepush"
  },
  "dependencies": {
    "@swc/helpers": "^0.3.2",
    "axios": "^0.22.0",
    "contentful-resolve-response": "^1.3.0",
    "contentful-sdk-core": "^6.10.3",
    "json-stringify-safe": "^5.0.1",
    "regenerator-runtime": "^0.13.9"
  },
  "devDependencies": {
    "@contentful/browserslist-config": "^2.0.0",
    "@contentful/rich-text-types": "^15.3.6",
    "@parcel/packager-ts": "^2.2.1",
    "@parcel/transformer-typescript-types": "^2.2.1",
    "@semantic-release/changelog": "^5.0.1",
    "@types/contentful-resolve-response": "^0.1.31",
    "@types/jest": "^27.0.2",
    "@types/json-stringify-safe": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "bundlesize": "^0.18.1",
    "contentful-sdk-jsdoc": "^2.2.0",
    "core-js": "^3.2.1",
    "cz-conventional-changelog": "^3.0.2",
    "es-check": "^5.2.4",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-config-standard": "^16.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "express": "^4.15.4",
    "fast-copy": "^2.1.0",
    "husky": "^4.3.8",
    "in-publish": "^2.0.0",
    "istanbul": "^1.0.0-alpha.2",
    "jest": "^27.4.7",
    "jest-when": "^3.0.1",
    "json": "^11.0.0",
    "mkdirp": "^1.0.3",
    "nodemon": "^2.0.2",
    "parcel": "^2.2.1",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.0",
    "semantic-release": "^17.0.4",
    "ts-jest": "^27.1.3",
    "tsd": "^0.17.0",
    "tslib": "^2.0.3",
    "type-fest": "^0.20.2",
    "typedoc": "^0.22.4",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": ">=12"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "bundlesize": [
    {
      "path": "./dist/contentful.esm.js",
      "maxSize": "35Kb"
    },
    {
      "path": "./dist/contentful.esm.min.js",
      "maxSize": "25Kb"
    },
    {
      "path": "./dist/contentful.cjs.js",
      "maxSize": "45Kb"
    },
    {
      "path": "./dist/contentful.cjs.min.js",
      "maxSize": "32Kb"
    }
  ],
  "release": {
    "branches": ["master", "next"],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "releaseRules": [
            {
              "type": "build",
              "scope": "deps",
              "release": "patch"
            }
          ]
        }
      ],
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/changelog",
      "@semantic-release/github"
    ]
  }
}
